# NPM
alias ns='npm start'
alias nt='npm test'
alias ni='npm i'

# Git
alias gs='git status'
alias gc='git checkout'
alias gb='git branch'
alias ga='git add .'
alias gd='git diff'
alias gds='git diff --staged'
alias gl='git log --graph --oneline --decorate=short --all --color=always | head -n $LINES'
alias glive='watch --color --interval 1 "git log --graph --oneline --color=always --decorate=short --all"'
alias gporigin='git push -u origin $(git symbolic-ref --short HEAD)'
alias gap='git add -p'
alias grp='git reset -p'
alias gcp='git checkout . -p'
alias gom='git commit'

# Nvim
alias nv='nvim'

